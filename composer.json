{
    "name": "matrixbrains/laravel-form-sync",
    "description": "Sync Laravel validation rules & translations with frontend (React/Vue).",
    "keywords": [
        "laravel",
        "react",
        "validation",
        "forms",
        "sync"
    ],
    "homepage": "https://github.com/matrixbrains/laravel-form-sync",
    "license": "MIT",
    "authors": [
        {
            "name": "Vikram Mevasiya",
            "email": "vikram.mevasiya@matrixbrains.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Matrixbrains\\FormSync\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Matrixbrains\\FormSync\\Providers\\FormSyncServiceProvider"
            ]
        }
    }
}
